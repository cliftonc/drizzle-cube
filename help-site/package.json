{
  "name": "drizzle-cube-help-site",
  "version": "0.1.3",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run build:help-content\" \"vite\"",
    "build": "npm run build:help-content && vite build && vite build --config vite.config.worker.ts",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler deploy",
    "deploy:staging": "npm run build && wrangler deploy --env staging",
    "build:help-content": "node scripts/build-help-content.js",
    "watch:help-content": "node scripts/watch-help-content.js"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "@heroicons/react": "^2.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "vite": "^5.4.1",
    "typescript": "^5.5.3",
    "marked": "^14.1.0",
    "concurrently": "^8.2.2",
    "chokidar": "^4.0.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "wrangler": "^3.78.12"
  }
}